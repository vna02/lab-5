#include <iostream>
using namespace std;

int main() {
	char c,a,e,i,o,u,A,E,I,O,U;
cout<<"enter an alphabet";
cin>>c;
if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c =='u' || c == 'A' || c == 'E' ||  c== 'O' || c == 'I' || c == 'U')
{
cout<<" the alphabet is a vowel";
}
else
{
cout<<"the alphabet is a consonant";
}
	return 0;
